    "file-loader": "^0.8.4",
    "stats-webpack-plugin": "^0.2.2"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "body-parser": "^1.14.1",
    "classnames": "^2.1.5",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "history": "^1.12.4",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
